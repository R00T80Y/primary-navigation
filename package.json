    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.16.4",
    "babel-loader": "^8.2.3",
    "core-js": "^3.19.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.4.0",
    "html-webpack-plugin": "^5.5.0",
