  "version": "1.2.0",
  "description": "Menus on checkboxes",
  "main": "./dist/PrimaryNavigation.js",
  "module": "./src/PrimaryNavigation.js",
  "scripts": {
    "server": "http-server ./dist -a localhost -p 9000 -c-1 -o",
    "serve": "cross-env NODE_ENV=development webpack serve --open --config tasks/webpack.dev.js",
