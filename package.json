    "build:prod": "cross-env NODE_ENV=production webpack --config tasks/webpack.prod.js",
    "build:polyfills": "cross-env NODE_ENV=production webpack --config tasks/webpack.prod.polyfills.js",
    "pub": "npm run build && npm version patch && npm publish --access public"
